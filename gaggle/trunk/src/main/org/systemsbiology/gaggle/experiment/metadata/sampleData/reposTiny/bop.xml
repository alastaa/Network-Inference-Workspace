<?xml version="1.0"?>

<experiment name="bop knockout, light and oxygen" date="2004-03-01">

  <predicate category='species' value='Halobacterium NRC-1'/>
  <predicate category='strain'  value='wild type'/>
  <predicate category='perturbation' value='genetic:knockout:bop'/>

  <dataset status='primary' type='log10 ratios'>
    <uri> /users/pshannon/tomcat/server1/webapps/halo/data/bop.ratio </uri>
  </dataset>

  <dataset status='derived' type='lambdas'>
    <uri> /users/pshannon/tomcat/server1/webapps/halo/data/bop.lambda </uri>
  </dataset>

  <condition alias="bop__HO_D_vs_NRC-1">  
    <variable name="oxygen" value="High"/>
    <variable name="illumination" value="Dark"/>
  </condition>

  <condition alias="bop__HO_L_vs_NRC-1">  
    <variable name="oxygen" value="High"/>
    <variable name="illumination" value="Light"/>
  </condition>

  <condition alias="bop__LO_D_vs_NRC-1">  
    <variable name="oxygen" value="Low"/>
    <variable name="illumination" value="Dark"/>
  </condition>

  <condition alias="bop__LO_L_vs_NRC-1">  
    <variable name="oxygen" value="Low"/>
    <variable name="illumination" value="Light"/>
  </condition>

</experiment>

