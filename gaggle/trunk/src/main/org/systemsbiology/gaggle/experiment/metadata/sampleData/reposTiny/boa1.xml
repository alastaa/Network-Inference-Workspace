<?xml version="1.0"?>

<experiment name="boa1 knockout, light and oxygen" date="2004-03-01">

  <predicate category='species' value='Halobacterium NRC-1'/>
  <predicate category='strain'  value='delta boa1'/>
  <predicate category='perturbation' value='genetic:knockout:boa1'/>

  <dataset status='primary' type='log10 ratios'>
    <uri> /users/pshannon/tomcat/server1/webapps/halo/data/boa1.ratio </uri>
  </dataset>

  <dataset status='derived' type='lambdas'>
    <uri> /users/pshannon/tomcat/server1/webapps/halo/data/boa1.lambda </uri>
  </dataset>

  <condition alias="boa1__HO_D_vs_NRC-1">  
    <variable name="oxygen" value="High"/>
    <variable name="illumination" value="Dark"/>
  </condition>

  <condition alias="boa1__HO_L_vs_NRC-1">  
    <variable name="oxygen" value="High"/>
    <variable name="illumination" value="Light"/>
  </condition>

  <condition alias="boa1__LO_D_vs_NRC-1">  
    <variable name="oxygen" value="Low"/>
    <variable name="illumination" value="Dark"/>
  </condition>

  <condition alias="boa1__LO_L_vs_NRC-1">  
    <variable name="oxygen" value="Low"/>
    <variable name="illumination" value="Light"/>
  </condition>

</experiment>

