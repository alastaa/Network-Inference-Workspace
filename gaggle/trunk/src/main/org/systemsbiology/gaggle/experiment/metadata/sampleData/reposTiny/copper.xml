  <dataset status='derived' type='lambdas'>
    <uri> /users/pshannon/tomcat/server1/webapps/halo/data/copper.lambda </uri>
  </dataset>

  <condition alias='Cu_1_vs_C.sig'>
    <variable name='concentration' value='700' units='micromolar'/>
