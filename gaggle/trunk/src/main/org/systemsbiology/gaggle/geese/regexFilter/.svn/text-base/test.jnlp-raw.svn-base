<?xml version="1.0" encoding="utf-8"?>
<jnlp
  codebase="$CODEBASE$"
  href="test.jnlp">
  <information>
    <title>RegexFilterGoose</title>
    <vendor> (January 2006)</vendor>
    <homepage href="docs/help.html"/>
    <offline-allowed/>
  </information>
  <security>
      <all-permissions/>
  </security>
  <resources>
    <j2se version="1.5+" max-heap-size="1024M"/>
    <jar href="regexFilterGoose.jar"/>
  </resources>
  <application-desc>
    <argument>--gooseName</argument>
    <argument>IPI</argument>
    <argument>--pattern</argument>
    <argument>IPI\d+?</argument>
  </application-desc>
</jnlp>

