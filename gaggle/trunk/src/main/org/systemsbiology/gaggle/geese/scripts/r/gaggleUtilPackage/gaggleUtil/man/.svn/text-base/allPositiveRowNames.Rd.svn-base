\name{allPositiveRowNames}
\alias{allPositiveRowNames}
\title{Find the Names of Rows in a Matrix Where All Values >= 0}
\description{Return the names of all rows in the supplied matrix in which all elements are greater than or
             equal to zero.}
\usage{positiveRowNames = allPositiveRowNames (matrix)}
\arguments{
  \item{matrix}{a matrix to be searched.}
  }

\value{Returns a list of row names.}
\examples{
  library (gaggleUtil)
  data (gamma)
  result = allPositiveRowNames (gamma.ratios)
  length (result) # 81 rows have all-positive values
  result [1:5]    # display the first five row names:
                  # "VNG0040C" "VNG0177G" "VNG0258H" "VNG0285C" "VNG0286C"
}

\keyword{array}
\keyword{manip}
